<h1><center>Post An Ad<center></h1>
<div class="container">
  <div id="contact-area">
    <%= form_for @listing do |f| %>
        <%= f.label :title %>
        <%= f.text_field :title %>
        <%= f.label :description %>
        <%= f.text_area :description %>
        <%= f.label :city %>
        <%= f.text_field :city %>
        <%= f.label :province %>
        <%= f.text_field :province %>
        <%= f.label :zipcode %>
        <%= f.text_field :zipcode, class: 'zip-width' %>
        <%= f.submit class: 'create-button' %>
    <% end %>
  </div>
</div>